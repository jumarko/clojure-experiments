<?xml version="1.0" encoding="UTF-8"?>
<!--
     See https://github.com/clojure/tools.logging/blob/master/src/test/clojure/log4j2.xml
-->
<Configuration status="WARN">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d %-5p %c: %m%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="info">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>
